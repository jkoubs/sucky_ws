<?xml version="1.0" ?>
<sdf version="1.6">
  <world name="default">

    <!-- Load a light source -->
    <include>
      <uri>model://sun</uri>
    </include>

    <!-- Load the ground plane -->
    <include>
      <uri>model://ground_plane</uri>
    </include>

    <!-- Insert your apartment -->
    <include>
      <uri>model://zone-A</uri>
      <name>zone-A</name>
      <pose>0 0 0 0 0 0</pose> <!-- x y z roll pitch yaw -->
    </include>

  </world>
</sdf>