<?xml version="1.0"?>
<package format="3">
  <name>mpu9250driver</name>
  <version>0.0.1</version>
  <description>A ROS2 package that converts raw data from an MPU9250 to /imu format.</description>
  <maintainer email="hiwad.aziz@gmx.de">Hiwad Aziz</maintainer>
  <license>MIT</license>

  <buildtool_depend>ament_cmake</buildtool_depend>
  <exec_depend>rclpy</exec_depend>
  <exec_depend>sensor_msgs</exec_depend>
  <exec_depend>python3</exec_depend>

  <export>
    <build_type>ament_cmake</build_type>
  </export>
</package>
